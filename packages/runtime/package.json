{
  "name": "@makeswift/runtime",
  "version": "0.13.1",
  "license": "MIT",
  "files": [
    "dist",
    "prop-controllers",
    "react",
    "box-model",
    "components",
    "api",
    "next",
    "builder",
    "controls",
    "slate",
    "state"
