    "./controls": {
      "require": "./dist/cjs/controls/index.js",
      "import": "./dist/esm/controls/index.js",
      "types": "./dist/types/controls/index.d.ts"
    },
    "./slate": {
      "require": "./dist/cjs/slate/index.js",
      "import": "./dist/esm/slate/index.js",
      "types": "./dist/types/slate/index.d.ts"
