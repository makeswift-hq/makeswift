{
  "name": "@makeswift/runtime",
  "version": "0.9.7",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/src",
  "files": [
    "dist",
    "prop-controllers.js",
    "react.js",
    "box-model.js",
    "components.js",
    "api.js",
    "next.js",
    "next/plugin.js",
    "builder.js",
    "controls.js",
    "slate.js",
    "state/breakpoints.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./prop-controllers": {
      "import": "./dist/esm/prop-controllers/index.js",
      "require": "./dist/prop-controllers/index.js"
    },
    "./react": {
      "import": "./dist/esm/react.js",
      "require": "./dist/react.js"
    },
    "./box-model": {
      "import": "./dist/esm/box-model.js",
      "require": "./dist/box-model.js"
    },
    "./components": {
      "import": "./dist/esm/components/index.js",
      "require": "./dist/components/index.js"
    },
    "./api": {
      "import": "./dist/esm/api/index.js",
      "require": "./dist/api/index.js"
    },
    "./next": {
      "import": "./dist/esm/next/index.js",
      "require": "./dist/next/index.js"
    },
    "./next/plugin": "./next/plugin.js",
    "./builder": {
      "import": "./dist/esm/builder/index.js",
      "require": "./dist/builder/index.js"
    },
    "./controls": {
      "import": "./dist/esm/controls/index.js",
      "require": "./dist/controls/index.js"
    },
    "./slate": {
      "import": "./dist/esm/slate/index.js",
      "require": "./dist/slate/index.js"
    },
    "./state/breakpoints": {
      "import": "./dist/esm/state/breakpoints.js",
      "require": "./dist/state/breakpoints.js"
    }
  },
  "typesVersions": {
    "*": {
      "prop-controllers": [
        "dist/types/src/prop-controllers/index.d.ts"
      ],
      "react": [
        "dist/types/src/react.d.ts"
      ],
      "box-model": [
        "dist/types/src/box-model.d.ts"
      ],
      "components": [
        "dist/types/src/components/index.d.ts"
      ],
      "api": [
        "dist/types/src/api/index.d.ts"
      ],
      "next": [
        "dist/types/src/next/index.d.ts"
      ],
      "builder": [
        "dist/types/src/builder/index.d.ts"
      ],
      "controls": [
        "dist/types/src/controls/index.d.ts"
      ],
      "slate": [
        "dist/types/src/slate/index.d.ts"
      ],
      "state/breakpoints": [
        "dist/types/src/state/modules/breakpoints.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn clean && tsc --emitDeclarationOnly && tsup",
    "clean": "rm -rf dist",
    "dev": "conc -k 'tsc --emitDeclarationOnly --watch --preserveWatchOutput' 'tsup --watch'",
    "prepare": "yarn build",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "vitest run --config vite.test.config.ts",
    "test:watch": "vitest watch --config vite.test.config.ts",
    "madge": "madge --circular --warning --extensions ts,tsx src/ --exclude \"dist/.+\""
  },
  "peerDependencies": {
    "next": ">=12.2.0 <13.0.0 || ^13.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
