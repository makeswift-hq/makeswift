{
  "name": "@makeswift/runtime",
  "version": "0.0.0",
  "main": "dist/main.cjs",
  "module": "dist/main.es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js"
    },
    "./prop-controllers": {
      "import": "./dist/prop-controllers.es.js",
      "require": "./dist/prop-controllers.cjs.js"
    },
    "./react": {
      "import": "./dist/react.es.js",
      "require": "./dist/react.cjs.js"
    },
    "./box-model": {
      "import": "./dist/box-model.es.js",
      "require": "./dist/box-model.cjs.js"
    },
    "./components": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "prop-controllers": [
        "src/prop-controllers/index.ts"
      ],
      "react": [
        "src/react.ts"
      ],
      "box-model": [
        "src/box-model.ts"
      ],
      "components": [
        "src/components/index.ts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "prop-controllers": [
          "dist/types/prop-controllers/index.d.ts"
        ],
        "react": [
          "dist/types/react.d.ts"
        ],
        "box-model": [
          "dist/types/box-model.d.ts"
        ],
        "components": [
          "dist/types/components/index.d.ts"
        ]
      }
    }
  },
  "scripts": {
    "dev": "concurrently -k 'tsc --watch --preserveWatchOutput' 'vite build --watch'",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "styled-components": {
      "optional": true
    }
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "styled-components": "5.2.0"
  },
  "dependencies": {
    "@types/slate": "^0.47.8",
    "@types/styled-components": "^5.1.5",
    "@types/use-sync-external-store": "^0.0.3",
    "css-box-model": "^1.2.1",
    "immutable": "^4.0.0-rc.12",
    "ot-json0": "^1.1.0",
    "polished": "3.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "use-sync-external-store": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-react": "^1.1.4",
    "concurrently": "^5.3.0",
    "react": "^17.0.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.2"
  }
}
