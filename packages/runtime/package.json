{
  "name": "@makeswift/runtime",
  "version": "0.0.18",
  "license": "MIT",
  "main": "dist/main.cjs",
  "module": "dist/main.es",
  "types": "dist/types",
  "files": [
    "dist",
    "prop-controllers.js",
    "react.js",
    "box-model.js",
    "components.js",
    "api.js",
    "next.js",
    "builder.js",
    "controls.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js"
    },
    "./prop-controllers": {
      "import": "./dist/prop-controllers.es.js",
      "require": "./dist/prop-controllers.cjs.js"
    },
    "./react": {
      "import": "./dist/react.es.js",
      "require": "./dist/react.cjs.js"
    },
    "./box-model": {
      "import": "./dist/box-model.es.js",
      "require": "./dist/box-model.cjs.js"
    },
    "./components": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.cjs.js"
    },
    "./api": {
      "import": "./dist/api.es.js",
      "require": "./dist/api.cjs.js"
    },
    "./next": {
      "import": "./dist/next.es.js",
      "require": "./dist/next.cjs.js"
    },
    "./builder": {
      "import": "./dist/builder.es.js",
      "require": "./dist/builder.cjs.js"
    },
    "./controls": {
      "import": "./dist/controls.es.js",
      "require": "./dist/controls.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "prop-controllers": [
        "dist/types/prop-controllers/index.d.ts"
      ],
      "react": [
        "dist/types/react.d.ts"
      ],
      "box-model": [
        "dist/types/box-model.d.ts"
      ],
      "components": [
        "dist/types/components/index.d.ts"
      ],
      "api": [
        "dist/types/api/index.d.ts"
      ],
      "next": [
        "dist/types/next.d.ts"
      ],
      "builder": [
        "dist/types/builder/index.d.ts"
      ],
      "controls": [
        "dist/types/controls/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "concurrently -k 'tsc --watch --preserveWatchOutput' 'vite build --watch'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "yarn build",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "vitest"
  },
